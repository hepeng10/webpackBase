{
  "name": "webpack-base",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env ENV=PROD webpack --config webpack/webpack.config.prod.babel.js --progress --display-error-details",
    "build:dev": "cross-env ENV=DEV webpack --config webpack/webpack.config.prod.babel.js --progress --display-error-details",
    "build:test": "cross-env ENV=TEST webpack --config webpack/webpack.config.prod.babel.js --progress --display-error-details",
    "lint": "eslint -c .eslintrc.prod.json --ext .js,.jsx src/",
    "start": "cross-env ENV=DEV webpack-dev-server --inline --progress --config webpack/webpack.config.dev.babel.js",
    "start:test": "cross-env ENV=TEST webpack-dev-server --inline --progress --config webpack/webpack.config.dev.babel.js",
    "start:mock": "concurrently \"cross-env NODE_ENV=mock npm start\" \"node ./mock/server.js\""
  },
  "license": "ISC",
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "antd": "^3.26.14",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "file-saver": "^2.0.2",
    "get-object-field": "^1.0.7",
    "mobx": "^4.9.4",
    "mobx-react": "^6.1.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-loadable": "^5.5.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
